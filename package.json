{
  "name": "use_cases_book",
  "version": "1.0.0",
  "description": "A very simple use case administrator",
  "main": "/",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./dist/server.js",
    "initdb": "sqlite3 database.db < sql_scripts/create_db.sql; sqlite3 database.db < sql_scripts/fill_test_data.sql",
    "prepublish": "npm-run-all compile-coffee bundle",
    "clean-dist": "rm -rf ./dist && mkdir dist",
    "clean-public": "rm -rf ./public/js/*.js",
    "clean": "npm-run-all --parallel clean-dist clean-public",
    "compile-coffee-dist": "coffee -o ./dist --compile ./coffeescript/*.coffee",
    "compile-coffee-public": "coffee -o ./public/js --compile ./coffeescript/public/*.coffee",
    "compile-coffee": "npm-run-all compile-coffee-dist compile-coffee-public",
    "bundle": "webpack"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "body-parser": "^1.16.0",
    "coffee-script": "^1.12.6",
    "express": "^4.14.0",
    "npm-run-all": "^4.0.2",
    "sqlite3": "^3.1.8",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "express": "^4.14.0",
    "body-parser": "^1.16.0",
    "sqlite3": "^3.1.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dospro/use_cases_book.git"
  },
  "bugs": {
    "url": "https://github.com/dospro/use_cases_book/issues"
  },
  "homepage": "https://github.com/dospro/use_cases_book#readme"
}
